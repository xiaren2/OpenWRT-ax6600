From 1234567890 Mon Sep 17 00:00:00 2001
From: yourname <you@example.com>
Date: Sat, 9 Nov 2025 23:00:00 +0800
Subject: [PATCH] disable PHY LEDs on JDCloud RE-CS-02 (AX6600)

---
 target/linux/qualcommax/files/arch/arm64/boot/dts/qcom/ipq6010-re-cs-02.dts | 10 ++++++++++
 1 file changed, 10 insertions(+)

diff --git a/target/linux/qualcommax/files/arch/arm64/boot/dts/qcom/ipq6010-re-cs-02.dts b/target/linux/qualcommax/files/arch/arm64/boot/dts/qcom/ipq6010-re-cs-02.dts
index e69de29..abc1234 100644
--- a/target/linux/qualcommax/files/arch/arm64/boot/dts/qcom/ipq6010-re-cs-02.dts
+++ b/target/linux/qualcommax/files/arch/arm64/boot/dts/qcom/ipq6010-re-cs-02.dts
@@ -162,6 +162,16 @@
         qca8075_24: ethernet-phy@24 {
             compatible = "ethernet-phy-ieee802.3-c22";
             reg = <24>;
+            qca,disable-phy-leds;
         };
 
         qca8075_25: ethernet-phy@25 {
             compatible = "ethernet-phy-ieee802.3-c22";
             reg = <25>;
+            qca,disable-phy-leds;
         };
 
         qca8075_26: ethernet-phy@26 {
             compatible = "ethernet-phy-ieee802.3-c22";
             reg = <26>;
+            qca,disable-phy-leds;
         };
 
         qca8075_27: ethernet-phy@27 {
             compatible = "ethernet-phy-ieee802.3-c22";
             reg = <27>;
+            qca,disable-phy-leds;
         };
     };
 
@@ -175,6 +185,7 @@
         reg = <12>;
         reset-deassert-us = <10000>;
         reset-gpios = <&tlmm 77 GPIO_ACTIVE_LOW>;
+        qca,disable-phy-leds;
     };
 };
